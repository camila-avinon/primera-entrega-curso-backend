<!--Lista con todos los productos-->
<div>
    <h1>Lista de productos</h1>
    {{#each products}}
    <div>
        <h2>{{this.title}}<h2/>
        <h3>${{this.price}}</h3>
        <p>{{this.description}}</p>
        <p>{{this.category}}</p>
    </div>
    {{/each}}
</div>
<script src="/socket.io/socket.io.js"></script>
<script src="/js/index.js"></script>